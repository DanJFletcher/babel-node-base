{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "off",
    "indent": ["error", "tab", { "SwitchCase": 1 }],
    "quotes": ["error", "single" ],
    "semi": ["error", "always"]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"]
}
